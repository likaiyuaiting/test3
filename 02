#include<stdio.h>
int main()
{
	int i,j=0,k,m;
	long int n;
	long double sum=1;
	for(i=1;i<101;i++)
	{
		sum*=i;
	}
	for()
	k1=sum/10000000000000000000;
	k2=k1/10000000000000000000;
	k3=k2/10000000000000000000;
	k4=k3/10000000000000000000;	
	k5=k4/10000000000000000000;
	k6=k5/10000000000000000000;	
	k7=k6/10000000000000000000;
	k8=k7/1000000;
	m=(int)k5;
	printf("%lf\t",k8);
	while(m)
	{
	if(m%10!=0) break;	
	m=m/10;
	j++;
	}
	printf("%lf\n%d\n",sum,j);
	return 0;
}
