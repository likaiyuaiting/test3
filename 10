#include<stdio.h>
#include<string.h>
int main(void)
{
	string ch;
	int i,j,k,n,n1=0,n2=0,n3=0;
    //for(i=0;i<2000;i++)
		scanf("%s",&ch);

    for(i=0;i<2000;i++)
		if(ch[i]=='a')	
			n1++;

	for(j=n1;j<2000;j++)
		if(ch[j]=='c')
			n2++;

	for(k=n1+n2;k<2000;k++)
		if(ch[k]=='m')
			n3++;
	printf("%d",n1*n2*n3);
	return 0;
}
